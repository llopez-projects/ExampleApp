{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ExampleApp;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "CacheSettings": {
        "DefaultExpirationMinutes": 5,
        "EmployeesExpirationMinutes": 3
    },
    "ElasticLogging": {
        "Enabled": true,
        "Uri": "http://localhost:9200",
        "IndexFormat": "exampleapp-endpoints",
        "ApplicationName": "ExampleApp"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
